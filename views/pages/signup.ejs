    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/stylesheets/bootstrap.min.css">
    <link rel="stylesheet" href="/stylesheets/style.css">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Crypto-currency</title>
</head>
<body>
<!-- Navigation -->
<nav class="navbar navbar-expand-lg navbar-light bg-light">
  <a class="navbar-brand" href="#">
        <img src="/images/crypto.png" height="50" class="d-inline-block align-top" alt="">
    </a>
  <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>
  <div class="collapse navbar-collapse" id="navbarNav">
    <ul class="navbar-nav">
      <li class="nav-item">
        <a class="nav-link" href="/">Acheter</a>
      </li>
      <li class="nav-item">
        <a class="nav-link" href="/sell">Vendre</a>
      </li>
      <li class="nav-item">
        <a class="nav-link" href="/pricing">Prix</a>
      </li>
      <li class="nav-item active">
        <a class="nav-link" href="/about">Informations <span class="sr-only">(actuellement)</span></a>
      </li>
    </ul>
  </div>
