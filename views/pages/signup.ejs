<!-- Container -->
<div class="container">
    <br/>
    <div class="card">
        <div class="card-body">
            <h6 class="card-subtitle mb-2 text-muted text-uppercase">Inscription</h6>
            <form method="post" style="margin-bottom: -5px">
                <div class="input-group mb-3">
                    <input type="text" class="form-control" aria-label="Amount (to the nearest dollar)" name="username">
                </div>
                <small class="form-text text-muted" style="margin-top: -15px; margin-bottom: 10px">Entrez votre prénom, suivi de votre numéro de classe (ex: Axel11)</small>
                <button type="submit" class="btn btn-block btn-info" id="sell-btn">Créer mon compte</button>
            </form>
        </div>
    </div>
</div>

<script src="/javascript/jquery-3.2.1.slim.min.js"></script>
